WHILE TRUE
	KBD(VK_NEXT,CLICK,1000)
	SLEEP(1)

	FOR Y = 1 TO 3
		KBD(VK_LEFT,DOWN)
		FOR X = 1 TO 18
		KBD(VK_CTRL,CLICK,1300)
		NEXT
		KBD(VK_LEFT,UP)
	
		KBD(VK_RIGHT,DOWN)
		FOR X = 1 TO 18
		KBD(VK_CTRL,CLICK,1300)
		NEXT
		KBD(VK_RIGHT,UP)
	NEXT
WEND



