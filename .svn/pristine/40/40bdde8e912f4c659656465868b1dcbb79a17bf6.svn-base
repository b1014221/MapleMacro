function checkFamiliarPower()
	
	KBD(VK_F6,0,800)
	SLEEP(1)
	if chkimg("../picture/fam500以下.bmp",0,0,0,1368,800) then KBD(VK_4,0,500)
	KBD(VK_F6,0,700)
	

	result = 0
fend
